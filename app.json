{
  "expo": {
    "name": "Pay & Promise",
    "slug": "pay-and-promise",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "payandpromise",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ashbin.payandpromise",
      "splash": {
        "backgroundColor": "#EDE7F6",
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#EDE7F6",
          "image": "./assets/images/splash.png",
          "resizeMode": "contain"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "upi"
            }
          ]
        }
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/splash.png"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#EDE7F6",
          "dark": {
            "image": "./assets/images/splash.png",
            "imageWidth": 300,
            "resizeMode": "contain",
            "backgroundColor": "#EDE7F6"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.20"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a65f26e7-88d5-4327-9978-e886690a7eda"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a65f26e7-88d5-4327-9978-e886690a7eda"
    }
  }
}